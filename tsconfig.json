{
  
  "compilerOptions": {
    "isolatedModules":true,
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "types": ["vite/client"]
  },
  "include": ["./src"]
}
